{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "skipLibCheck": true,
        "noImplicitAny": false,
        "paths": {
            "@/*": ["./*"]
        }
    },
    "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"],
    "exclude": ["node_modules", "**/*.spec.ts", "**/*.test.ts"]
}
